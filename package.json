    "@azure/storage-file-share": "^12.8.0",
    "@azure/storage-queue": "^12.7.0",
    "body-parser": "^1.19.1",
    "command-line-args": "^5.2.0",
    "compression": "^1.7.4",
    "consolidate": "^0.16.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "eol": "^0.9.1",
    "express": "^4.17.2",
    "express-http-context": "^1.2.4",
    "express-jwt": "^6.1.0",
