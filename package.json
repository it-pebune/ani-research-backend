{
  "name": "main-apis",
  "version": "0.5.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts .",
    "build": "npm --no-git-tag-version version patch && node ./util/build.js",
    "deploy": "../utils/deploy-backend.sh",
    "start": "node ./dist",
    "dev": "nodemon --config nodemon.json",
