    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mssql": "^7.3.0",
    "multer": "^1.4.4",
    "node-cleanup": "^2.1.2",
    "uuid": "^8.3.2",
    "winston": "^3.4.0",
    "winston-transport": "^4.4.2"
