    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "apidoc": "^0.50.3",
    "edit-json-file": "^1.7.0",
    "eslint": "^8.7.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "fs-extra": "^10.0.0",
